license=('GPL')
source=("https://github.com/amylum/${_realname}/releases/download/${pkgver//_/-}/${_realname}.tar.gz")
noextract=("${_realname}.tar.gz")
sha512sums=("595e6cd901ecc41c6462eca25f1930c7469cff3a2e72ee83411689633aca1703abdb02ae6b0ef56139791f610d765ce320d9068e95fb9c942b9846ca0f7743a2")

