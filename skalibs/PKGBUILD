pkgname=skalibs
pkgver=2.6.4.0_1
pkgrel=1
pkgdesc="general-purpose libraries for skarnet packages"
arch=('x86_64')
url="https://github.com/amylum/skalibs"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("37bcd3bda50be6a01c0dd31a643b19b783d9eb0133b281028ede48d30321eb3449924b6a7c17b2ba0d96ed43f0264638488d20fb57545fddca617d089aeeb9e6")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
