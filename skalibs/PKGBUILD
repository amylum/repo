pkgname=skalibs
pkgver=2.6.1.0_1
pkgrel=1
pkgdesc="general-purpose libraries for skarnet packages"
arch=('x86_64')
url="https://github.com/amylum/skalibs"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("8e14682581487f5eb9eff04e627253ca9aebf424745d11ac45d02743abda55f93f134057897f7cb08fd47a2f0467d1cfdee82cda2fda440b32b36cba2b13a186")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
