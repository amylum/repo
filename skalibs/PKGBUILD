license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("7c814eb6a28cc214be5dbb8df4ce00a5006d8f20da9a9918f772a977714904a5aa2d038b9b2d44e3a518e8d283492a6d26cb43abe8597809c43cd98444f8a544")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
