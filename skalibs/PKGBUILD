pkgname=skalibs
pkgver=2.7.0.0_1
pkgrel=1
pkgdesc="general-purpose libraries for skarnet packages"
arch=('x86_64')
url="https://github.com/amylum/skalibs"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("f5e66f5efc194ef280dda38c524f239d55a0cf4eb11d0c05f9a15fc0d75c0ca21d591f9b980deb19de7722c8f12c8dca6cafb93f49afb89fc6f783f0f8b130fd")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
