pkgname=skalibs
pkgver=2.6.0.1_1
pkgrel=1
pkgdesc="general-purpose libraries for skarnet packages"
arch=('x86_64')
url="https://github.com/amylum/skalibs"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("69a6e66c1b5ccf6e68ce33e315e748e0784c38146f571911b544d83eeea45fa61843c8516e82811a539c8776e2bf2696c9eab253cfbe2e489d72b19060ba5efe")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
