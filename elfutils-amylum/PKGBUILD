# Maintainer: Les Aker <me@lesaker.org>
pkgname=elfutils-amylum
_realname=elfutils
pkgver=0.172_1
pkgrel=1
pkgdesc="Utilities to handle ELF object files and DWARF debugging information"
arch=('x86_64')
url="https://github.com/amylum/elfutils"
license=('GPL3')
source=("https://github.com/amylum/${_realname}/releases/download/${pkgver//_/-}/${_realname}.tar.gz")
noextract=("${_realname}.tar.gz")
sha512sums=("a7864bd21d7857f946b0717b05dc8593e4e5d677ceb5cbbbc8c6fcdd4bbefb50317f62d80e8ed5bf1c94ba3aaf02a56995abf23a97c3e258476bb30c678963a6")

conflicts=("${_realname}")
provides=("${_realname}")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${_realname}.tar.gz"
    mv usr/share/licenses/${_realname} usr/share/licenses/${pkgname}
}
