# Maintainer: Les Aker <me@lesaker.org>
pkgname=elfutils-amylum
_realname=elfutils
pkgver=0.170_1
pkgrel=1
pkgdesc="Utilities to handle ELF object files and DWARF debugging information"
arch=('x86_64')
url="https://github.com/amylum/elfutils"
license=('GPL3')
source=("https://github.com/amylum/${_realname}/releases/download/${pkgver//_/-}/${_realname}.tar.gz")
noextract=("${_realname}.tar.gz")
sha512sums=("78a0a5ad53cd1f99907c7e782f0a5c3e5ef61a11ae1789369b377bf96369b4e7a6d9db8ab0799ca89cbf02d73199a467da70d4129ac1c95286d9252e2314c3f4")

conflicts=("${_realname}")
provides=("${_realname}")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${_realname}.tar.gz"
    mv usr/share/licenses/${_realname} usr/share/licenses/${pkgname}
}
