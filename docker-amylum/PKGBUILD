pkgname=docker-amylum
_realname=docker
pkgver=1.7.1_17
pkgrel=1
pkgdesc="application container engine"
provides=("${_realname}")
conflicts=("${_realname}")
arch=('x86_64')
url="https://github.com/amylum/docker"
license=('Apache')
source=("https://github.com/amylum/${_realname}/releases/download/${pkgver//_/-}/${_realname}.tar.gz")
noextract=("${_realname}.tar.gz")
sha512sums=("627834409bf230414cc8d4d306524fd8c83c9bc72d9c28627a610f144d749ddefb2e75b90294f3a35116727608fbebb8a2153ec8904f96848bef2e1eaf5ac1dc")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${_realname}.tar.gz"
}
