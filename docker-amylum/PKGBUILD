pkgname=docker-amylum
_realname=docker
pkgver=1.7.0_15
pkgrel=1
pkgdesc="application container engine"
provides=("${_realname}")
conflicts=("${_realname}")
arch=('x86_64')
url="https://github.com/amylum/docker"
license=('Apache')
source=("https://github.com/amylum/${_realname}/releases/download/${pkgver//_/-}/${_realname}.tar.gz")
noextract=("${_realname}.tar.gz")
sha512sums=("437fadd269accbcd53fb3ac008fea5515a5277c84c7cfe238d83ee4e303d2fd589636c2115743dde86e7c87837fb0fcc15a97791273dc2c4e1e7c8d09aeef7fa")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${_realname}.tar.gz"
}
