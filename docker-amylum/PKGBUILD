pkgname=docker-amylum
_realname=docker
pkgver=1.8.0_rc3_18
pkgrel=1
pkgdesc="application container engine"
provides=("${_realname}")
conflicts=("${_realname}")
arch=('x86_64')
url="https://github.com/amylum/docker"
license=('Apache')
source=("https://github.com/amylum/${_realname}/releases/download/${pkgver//_/-}/${_realname}.tar.gz")
noextract=("${_realname}.tar.gz")
sha512sums=("70ee62250df08971c0bce8267c7ffebbcfa235e4345947dc2928aeecb8ffb9805fbd0cfcd56b32215fea4bea83de7f44339732e2b92f0403237c68da270ad61a")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${_realname}.tar.gz"
}
