source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("14d5abfe65ca2d1495e2b0c039c1d4dec783eff782e8a1d2b28b9fe9256a10c54847a28378c3823241d8d138a574b65102b23f3dfaa8d4970207028e60e1ac77")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
