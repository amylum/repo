noextract=("${pkgname}.tar.gz")
sha512sums=("372a6375701d227be41371adac1aacb0d9b3dc0a4158e6689d96174ff5561de91f579b1af0f5e8f96a30ce60bbf045b11e9949e458f84d59a605dccb9e04357b")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
