license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("781f6eb6db9f3dcc8b952b12d81fb19386a903ed4a8a2c9108f01a306a24df53fb84bbe9eb86982f954c7c49cd2edbced06634d47b50d0303c8617788e33d203")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
