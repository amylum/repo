sha512sums=("08ad9ea9e72810f6084f068ffb8e3c7c5f5556df154b8f56e3e669c9dbbaa203390efa179b756135ce6481e5bf0cebf8f79644eee5b7e0b21463b55cfd7a0191")

package() {
    cd "${pkgdir}"
