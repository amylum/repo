noextract=("${pkgname}.tar.gz")
sha512sums=("381f64c19c1d5ac22fb0b9f134f6fc5546f20c6c0ad069e4d620ea1dabb9c73e879c08e1e4713e3e29d7b0c73e33b2b2b6d69bc65142a949734a20eeecf73db1")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
