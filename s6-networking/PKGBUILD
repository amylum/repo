noextract=("${pkgname}.tar.gz")
sha512sums=("66b59d92135c0cf63deee930100a5684e7430afe0f25b19876935b4da68b5ea7c3038aca3f24d4437a3135b86750bd85b2283cd101661423858b96a1c51f19a8")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
