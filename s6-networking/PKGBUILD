pkgname=s6-networking
pkgver=2.3.0.2_4
pkgrel=1
pkgdesc="small networking utilities for Unix"
arch=('x86_64')
url="https://github.com/amylum/s6-networking"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("1500a206dd28d1248d85ec67e0fa4802747d85f98995d10e088609f563170a8c2b5dee5e23e4dcbd484a3d8b7a22b8cd2cef69f117c6711e9c8e10552e259292")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
