noextract=("${pkgname}.tar.gz")
sha512sums=("d5959d108e2a575d69f8c42e67ac623eeed05a8b834948c773d23f9a837f4e85f29d997b69bee05d911662c473bcf93c28eaa1955895a0b96e2d266f5f80e678")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
