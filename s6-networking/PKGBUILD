noextract=("${pkgname}.tar.gz")
sha512sums=("135a4b8b5c155d795e9c0988bfd261c93e52fea6386d8c4d9e2bbcbc8ea1cbcc7a91832704121a9bb49217ee7b39940cffc3184226760c5419ff392af69455b3")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
