license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("1c4c69d2e8bd05f388f82623c742b65fc31e9ed592976236b91f73ec596b52369f44ef0e64d72c61421d9b48b98744ae0e118e4334ad69f477d158a602a160a2")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
