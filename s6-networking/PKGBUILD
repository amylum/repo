license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("4fbfb92ca184e3f39544fe2647f462fffa1bc10ac6bbc1de5d5b9689ca82fd87f7e55f1946fa74816874adf4290f1d14e834609d2140587d7743a50befb86983")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
