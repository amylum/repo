noextract=("${pkgname}.tar.gz")
sha512sums=("95de4af778052ab34186191c2ce58fda7eeba79114c0c9331cf7d603a7c7f44e62e8bcdd32e3ce583ac6c0fc9bb715a814459631ab7683aa624e53c45553d951")

package() {
    cd "${pkgdir}"
