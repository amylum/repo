noextract=("${pkgname}.tar.gz")
sha512sums=("940991693106f1aa978ffba8019a48e9b564d8f2003dae3bf509cebf3a9004a1666867f39fe6ac4c91d4ab73f0f90c9625aced3af76ca73ec0bac1e4601538fc")

package() {
    cd "${pkgdir}"
