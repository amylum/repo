noextract=("${pkgname}.tar.gz")
sha512sums=("01bd8f518edba0e0a7d0972176ae047c06ac54a8dee9f6296ce83281aa111fdd0acf7ab7ea7b27d446e272266e886694b0dffd1339efa62949a3f7c1fd64b931")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
