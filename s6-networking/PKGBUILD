source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("6e2332628dadd2fece6c619b8a3cec96bf398e567d0e008889c97644cff9b4f9f47f9f72009906d806d2a5e984a940376d7f7c6aa7f1b0e21dcda46683c1008d")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
