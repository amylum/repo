license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("d209ff8d2827e8dbe6f3662a52b67629c41bedb3ad630f7520d43f6a7c3f1a1b1719c828796045146cd65fb6c6c72dc27ccbb8a8a7a90b120727ab99d2348288")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
