noextract=("${pkgname}.tar.gz")
sha512sums=("f761e7fecc6b9a5fd5a9e2703b8285eea7eb6691cd5fbc190125f1e4d173a63e5e6808fd093deb88ea0cd90c0ea368e5f6ecb8bfdd7dd8912875f6a9d9dc4632")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
