sha512sums=("e2e11a51a45434acbf5dce34d64b1343801830e55cbdcb106e07921b27dc0069cd3abfbc12156098f6fff861ea5ca44607bc83d1f8a48b8be72c1273ebe14fe7")

package() {
    cd "${pkgdir}"
