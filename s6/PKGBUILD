sha512sums=("4f17c67491508d0e6bd71f099a656b4188f396617256803b5b8ed89ed51fe31c123113cca3870ef7d469da25f4c000fabf1dd84f4592a762cb029e63012260b1")

package() {
    cd "${pkgdir}"
