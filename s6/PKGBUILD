noextract=("${pkgname}.tar.gz")
sha512sums=("3d4c699a1b2b621dfcb6125eb98bea72bed31f57611e33b0d24c001d7948d1fdbf7b6f5b262174e99ad1d0fa6c1ea75fd4c06ac6e65a62f9677576eb24e1f604")

package() {
	cd "${pkgdir}"
