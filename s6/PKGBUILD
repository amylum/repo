noextract=("${pkgname}.tar.gz")
sha512sums=("8ed2d5c706adc5c6bf622018e7144cec473e15c900cb02bf3392c6bd32f5f2893da10f662511b69506c488c8ab56c07442f24ab40fc31748f7bb9ed092cd94cd")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
