noextract=("${pkgname}.tar.gz")
sha512sums=("296e2c2b9390e14cd157f7949b869071498be82cc5680310c8d5737fd1bf6b8a3186ad464624532e84b7ffeb052c9b745e70e1bd432378221d5f4fb11d2986e3")

package() {
	cd "${pkgdir}"
