sha512sums=("938dd719c644724d50b4ad33be0641a8220ff807bf00e915e21d6149bdd77294957709f5f6e379eacbe8492ca964a82dfd0e0f5810f4e2ade910d5a255a40535")

package() {
    cd "${pkgdir}"
