noextract=("${pkgname}.tar.gz")
sha512sums=("e10de40ba6ce2888b85525a4a650c9328917c03bde705a490f41eff3ed61abd42dbb63aee8708678c04413e3f9c653d36e0dfe6dde0c77dc387d62b55a3dd71b")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
