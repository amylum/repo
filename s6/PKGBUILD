sha512sums=("cbb736cc309cacbdc64736160f06944a506845bfa6c191124fe9373b75cc3dc789d3236ed518d34be8bac750595a75d076aacaaac065212db0623f94e90e4eeb")

package() {
    cd "${pkgdir}"
