noextract=("${pkgname}.tar.gz")
sha512sums=("6738ee1ec2a766182f914ed475422f53af603f959d49a268891d203ff8025aac4b611efa24078bbba07b4c89a030d80d93bdf42a5a2366dea1fe98cdfda53014")

package() {
    cd "${pkgdir}"
