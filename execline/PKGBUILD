pkgrel=2
pkgdesc="non-interactive scripting language"
arch=('x86_64')
url="https://github.com/amylum/execline"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("1e87d5c4f6f4dd64acc447c6351478fb7a36786e3d3884772c50b2688c0872ed9939b3f418175a548766961cd21206522943fca88e8e80eee7ff6210404d1e1e")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
