sha512sums=("871ec5355f4d6ddcb7119e95253438de828dded323ba82c82598dc234314c82029b11ec0ff5d5fec2aef40718a3a228b676310eddca3c074b7f82516e6397a65")

package() {
    cd "${pkgdir}"
