sha512sums=("b3e8d6432d03f4317477f0b1acb020a747b2e0a8d03d7771eb17a2eb8151058d04c31658e5481d474de716aa627f235f50afc03f96c9dce12cd27b74e411478f")

package() {
	cd "${pkgdir}"
