source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("ecb42ee82e84cad3e6e3909ef0db1152519e6654b47038c857bc90dce8ccac0089cca5f8d4fd7976b474b1d07ccb47acb431ea900b046dbd7d2ce72614b8b866")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
