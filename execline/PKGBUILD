noextract=("${pkgname}.tar.gz")
sha512sums=("08404fcc6dad464687a126423cec479516b07645c6183ce8e7cc51fc9954857115511616c9e71f5ba990cdea6a313fa67b7fc7d48396dd4a3b2b7375f714162d")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
