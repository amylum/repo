noextract=("${pkgname}.tar.gz")
sha512sums=("af325cc027c786526adc8a930e43ddfacd15da5f7216b97e04b0327bb2da7560ffe0200fd8e85feb47f85063ba8d134d0e53bd4e805d9542cc7539ec80a19306")

package() {
	cd "${pkgdir}"
