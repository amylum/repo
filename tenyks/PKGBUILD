noextract=("${pkgname}.tar.gz")
sha512sums=("556e74799264ccb071f9288e017c9d15b729104f970914491e619dbc521969fdcc8e8a7dc3971262c32e08fa1fa6d511b6087f3ea7c65cc4dd130e738e20f90a")

package() {
	cd "${pkgdir}"
