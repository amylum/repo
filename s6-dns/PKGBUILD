noextract=("${pkgname}.tar.gz")
sha512sums=("d9381a6e5e5be8bb9d3513a7ebd79f44a9585f3c6b40fb0f9721dbf5d642707b7542affe5acc16d09e4062616d1c36601d78079152fe2737d82b9b43dad98c9f")

package() {
    cd "${pkgdir}"
