pkgname=s6-dns
pkgver=2.2.0.0_7
pkgrel=1
pkgdesc="DNS client programs and libraries for Unix"
arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("0ae0c2e00cb954588c113a024c0704bec542c604454c96b0bb0d887d2b6da0d1f6a32e0b893b815f1e8bc36bbd5af82c0dde5ccabc55cc63d2fffb98507247e0")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
