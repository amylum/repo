arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("31711b6a749ece8328a3be3b174df10feab4e9500652303d9c28e104a2b091cee07e52ac93e26bfea519f3c8c00db37f1df4e45260a6081092935563a15384f4")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
