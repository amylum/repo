license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("1b9ce1cd27b1bfa60abf91143d3dd13fc06027a4c9acf18dc96121859ed118f7c3122f9e80c234be01d3a0d7439f1e15c593253ac3456bc52b52dbfb4ab16c89")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
