license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("d98aa8c56ee3a9c52dde4787581fd483a7478484966567b1a3bc84aad4a98fbf73feed10d819ceb59775495da522ae047cb856df35a57c97f94429359ac0e636")

package() {
    cd "${pkgdir}"
