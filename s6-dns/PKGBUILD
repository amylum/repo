pkgname=s6-dns
pkgver=2.2.0.1_1
pkgrel=1
pkgdesc="DNS client programs and libraries for Unix"
arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("4faefc588d1a2ace11deabb00fc334f545d7306d8e985aeababd02bf0237560bc73d19adbad2e632f02fc120966fe0d4de82385230f63d0f69ebc35ca4495978")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
