arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("d6f209b052f7b1344fb9a572997946340feb0650b1e6de9ecc1b7847a0330c686f6e4a556d3cc0fb5016b31465d813649b33181a7045d2dc7a3158aad6859c88")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
