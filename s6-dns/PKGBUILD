pkgname=s6-dns
pkgver=2.1.0.0_32
pkgrel=1
pkgdesc="DNS client programs and libraries for Unix"
arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("0f226dc8b22cffe6ca5cbb2f299905c36f5958b82f55022517b952860c27fa27da2f48ea253a7780d09fa94742b974529bc4ee8f692f3bd160420d4a01888e83")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
