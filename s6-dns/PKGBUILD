license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("8090f99bc4a4bbc8b4da4bc7ba372a17e3ed5d3e8e740d209259370829304e6b750dcb043d15a65d36c46b591a7111418cc629f1c3a408dcba5dcebe92727db4")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
