source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("7a7ee082a096b66774815d646024fad1e0563fb8a1746e4120bf65fa537053aa8e870b2a750623707e1fd1fab251a082ff28987cf2c3c4a33a78c4cbd7359c77")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
