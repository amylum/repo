source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("4e30efb03b84f58c7674f0bd2a3e9a29ad5d191cfabd1263bc9d3dc84bb7b04615f277a9fe007c685c27de3775a7e28ffc822ffce30abac8653e002f449feaef")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
