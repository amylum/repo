pkgname=s6-dns
pkgver=2.0.0.2_8
pkgrel=1
pkgdesc="DNS client programs and libraries for Unix"
arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("738c2d30f91ef0be2c9124b003d02a7959a6e209ca0c9bb02fe009a75a68104ca150068321c98b718f309f415d083a2b4108e5d50bd5e582bbb00fbf3fd81938")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
