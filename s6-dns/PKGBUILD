pkgname=s6-dns
pkgver=2.3.0.0_4
pkgrel=1
pkgdesc="DNS client programs and libraries for Unix"
arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("fc1e32f78d3306f9b6d8f34b506980cbe528d23c604be002c90a203411c66c4eb3f78e0b5a5df304f3b3d7d84906d5fcecceb6fa1dde58d93951ea73c93e5fb9")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
