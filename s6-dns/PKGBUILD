pkgname=s6-dns
pkgver=2.1.0.0_4
pkgrel=1
pkgdesc="DNS client programs and libraries for Unix"
arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("863b4ce1d0eb949c9d729a11c09e1d4106d999f9d0305992bee4222677d6e8192ca49e7de56034ab567d3f5f6b24233be47dfb23bc52e845c2d79ca2d54e351f")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
