source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("6d2a5debcf3fb563f10afcd0ddf224dfc78bda3f3ca3e4df5c8e83559ca04efc6c08d7c2d5d4840adfb198757e3e1886bbe34a952aa0b3d7c79b6347daa4e3d8")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
