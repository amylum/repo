source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("14b4043ba9e972b3ce1beec79ded10836bfeaf39ea431e6f21c5b138b599c3dda5989ad2486665b0d3d621e080e784faa85c8bf2f348aeafc48313ec8b294cb9")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
