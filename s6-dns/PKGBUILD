noextract=("${pkgname}.tar.gz")
sha512sums=("5e64fd81689d63a5aedae8f8d4247de0f6f3966a0f479cabedb4508b93f2ff33891ee9be54b2e6eb5f06b9e8a4fbd65de35a3f82f52e24b04705efb739bbce89")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
