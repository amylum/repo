noextract=("${pkgname}.tar.gz")
sha512sums=("ce0818df528893a2ed6dbcd638963ace6b33aa4a0cbb0eb51c41073edcfe32c7f3d4e3e4b6aea66a4cee100630b9950592bf1947aa448131c34066f75f68d39f")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
