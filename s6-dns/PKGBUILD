arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("1a25be778da6b5551e26937e3099c6dca398a5ae5436b059b3ba9621a6fb39782978616dbe2265afa386bea3c3df9f113344753724a5f4fce4eab90b11e9ae56")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
