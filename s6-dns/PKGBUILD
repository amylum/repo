pkgname=s6-dns
pkgver=2.0.0.7_20
pkgrel=1
pkgdesc="DNS client programs and libraries for Unix"
arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("ae02687c9cdb0f05017d8500d198596b86cdb9cc7206572869db341798cd87f14433bbcf901175cc1ad828f212bd243babee284adb5a727c92b95a634b98d8d2")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
