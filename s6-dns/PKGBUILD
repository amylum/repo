source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("4b26537485fd042ccf96a7c61b055269fb2b3af2a1ec490e9249f1cee4507b2bb0adc29ae94000ffb8ba9d807ebc3f0b0efcd0b2004386a501f3871810366b2f")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
