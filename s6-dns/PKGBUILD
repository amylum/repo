license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("2b05e04b8c710c844af4a3062ceb0507c95dabf6a3ef070e7aee48f4afbdd5b57120eb25e009d6f5cdb8430faa32660950ec6c236a2bba3f1153218a1e51c69b")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
