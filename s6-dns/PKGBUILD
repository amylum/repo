source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("39dc757fb5d7d7c73e0543a1eda11f6ddddae6dd2fb9ffe3583ff3ccf924bc64a18e5df8d5f241fcdd41efc20d39781ede9f517e8d9104876b34616a136abec4")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
