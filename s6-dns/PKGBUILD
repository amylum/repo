license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("bb4b00cef540144573940bfa4e4742952804380be4a85565e4ef2481be1056d5e8208b323d47018510b493e687a985101a7f1c7e98517bdddc9585f3a3866431")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
