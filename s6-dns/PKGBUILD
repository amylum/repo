license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("e7bb9a552de7691d6a9ec36688ba530003d38fa084464f6e29ed36c37806abcab68a3dc40662f40fe4e221f54a2678b4c3cca762056eeea9ca29142df760221c")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
