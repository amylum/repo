arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("cd37167f67c172f3e886ba8102ea36ba66a10753a9cb1c8659b96c611008e8c0012430b14a4a6ac811f852e7658e61bd96ea84efce2af7b31cc90c3e965fa64a")

package() {
	cd "${pkgdir}"
	tar xf "${srcdir}/${pkgname}.tar.gz"
}
