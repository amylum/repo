pkgname=s6-dns
pkgver=2.1.0.0_28
pkgrel=1
pkgdesc="DNS client programs and libraries for Unix"
arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("8a692d0be4f618806bf99ed6986f6228fb601998677fa96e7250006893982d442786ee55eb2575041f93b9557a9d4bb11feb4c22b091dcd2a57e618bd995e602")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
