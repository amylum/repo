license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("eab4c4aa0a64358e1e456f2bdc4e19c23e320b5a7fffbde4795bca0fb9eadbc61336476a458ad4aa1c302a67995c748151f9ed4317ca604f5412fcda2c428d8d")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
