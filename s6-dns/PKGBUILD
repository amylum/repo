source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("a9dabd28e5e9b972c2a808515bd60324b971ab555583094b4c0ded63e1e77df367cb78755d870e2ec57070e90f4ffd25c8917836ab6840241f9ba3d37120a0b6")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
