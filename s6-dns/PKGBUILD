license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("e3757592f573b86e9d7476c0e4e457ef1f4fb2889c9b737d73aebc1b3715ecfb0d1e0a5b79a52ada9cb18e05625b4aefc54eb82b97146186e915aec17feb4bb7")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
