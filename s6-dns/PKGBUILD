noextract=("${pkgname}.tar.gz")
sha512sums=("7ac81bce40b59839faefaba1dea76866eb121df766fb7571cdbae7ec07e49a18265d51f7e3f234fd41afd6d30267840f279ca277f7e3b321a50f0c79222df6f6")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
