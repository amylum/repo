pkgname=s6-dns
pkgver=2.2.0.1_4
pkgrel=1
pkgdesc="DNS client programs and libraries for Unix"
arch=('x86_64')
url="https://github.com/amylum/s6-dns"
license=('custom:ISC')
source=("https://github.com/amylum/${pkgname}/releases/download/${pkgver//_/-}/${pkgname}.tar.gz")
noextract=("${pkgname}.tar.gz")
sha512sums=("3d2a38009c16e349616c326053dbb35cd6fc05b6bfc518a48dbe1f46c573e525c01e9443ed1e89d4fdcd53aea12401fca361726ba0785c46924eabe1bb381db9")

package() {
    cd "${pkgdir}"
    tar xf "${srcdir}/${pkgname}.tar.gz"
}
