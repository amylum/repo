noextract=("${pkgname}.tar.gz")
sha512sums=("1e2df68fa3e83acfae91e0039441b6afa2e1e8836f4563e650349ec3b849e943631daefadb11fd71ef8a51a7560275da3a6fb7edcd9ce1c53a91fe7f8d901ae1")

package() {
    cd "${pkgdir}"
